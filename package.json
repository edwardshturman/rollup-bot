{
  "name": "rollup-bot",
  "author": "Edward Shturman",
  "bugs": "https://github.com/edwardshturman/rollup-bot/issues",
  "homepage": "https://github.com/edwardshturman/rollup-bot",
  "version": "0.3.1",
  "license": "UNLICENSED",
  "description": "Clean up your Discord channels via threading",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/edwardshturman/rollup-bot.git"
  },
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "standardx",
    "start": "node deploy-commands.js && node ."
  },
  "eslintConfig": {
    "rules": {
      "indent": [
        "error",
        4
      ],
      "semi": [
        "error",
        "always"
      ],
      "curly": [
        "off",
        "always"
      ],
      "brace-style": [
        "off",
        "always"
      ]
    }
  },
  "dependencies": {
    "@discordjs/builders": "^0.15.0",
    "@discordjs/rest": "^0.5.0",
    "discord-api-types": "^0.35.0",
    "discord.js": "^13.3.1",
    "dotenv": "^10.0.0"
  },
  "devDependencies": {
    "standardx": "^7.0.0"
  },
  "engines": {
    "node": "16.9.1"
  }
}
